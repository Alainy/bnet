{
    "name": "pwnraid/bnet",
    "description": "Library for Battle.net Web APIs",
    "type": "library",
    "keywords": ["Battle.net", "Blizzard", "Warcraft", "Starcraft", "Diablo"],
    "homepage": "https://github.com/pwnraid/bnet",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jyggen@pwnraid.org",
            "homepage": "https://pwnraid.org"
        },
        {
            "name": "pwnRaid Community",
            "homepage": "https://github.com/pwnraid/bnet/graphs/contributors"
        }
    ],
    "support": {
       "issues": "https://github.com/pwnraid/bnet/issues",
       "source": "https://github.com/pwnraid/bnet"
   },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0",
        "league/oauth2-client": "~1.0",
        "tedivm/stash": "0.13.*",
        "sebastian/version": "~1.0"
    },
    "require-dev": {
        "apigen/apigen": "^4.1.1",
        "jakub-onderka/php-parallel-lint": "^0.9",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "^0.7",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {"Pwnraid\\Bnet\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Pwnraid\\Bnet\\Test\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
