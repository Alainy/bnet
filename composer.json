{
    "name": "boo/bnet",
    "description": "Library for working with the Battle.net Web APIs.",
    "keywords": ["Battle.net", "Blizzard", "Warcraft", "Starcraft", "Diablo", "API", "bnet"],
    "homepage": "https://github.com/jyggen/bnet",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jonas@stendahl.me"
        }
    ],
    "support": {
        "docs": "https://docs.jyggen.com/boo-bnet/latest",
        "issues": "https://github.com/jyggen/bnet/issues",
        "source": "https://github.com/jyggen/bnet"
   },
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2.1",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpmd/phpmd": "^2.6.0",
        "phpro/grumphp": "^0.11.4",
        "phpstan/phpstan": "^0.6.4",
        "phpunit/phpunit": "^6.1.0",
        "sami/sami": "^4.0.1",
        "sebastian/phpcpd": "^3.0.0",
        "symfony/var-dumper": "^3.2.7"
    },
    "autoload": {
        "psr-4": {
            "Boo\\BattleNet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boo\\BattleNet\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "test": [
            "mkdir -p build/logs",
            "grumphp run"
        ]
    }
}
